//导出数据
function exportTrack(basePath){
		var $input = $("<input type='hidden' name='paramsTable'/>");
		var $titles=$("<input type='hidden' name='titles' />");
		var titles=getTitles();
		var data=getTableData();
        $input.attr('value',data);
        $titles.attr('value',titles);
        $("#ddform").append($input);
        $("#ddform").append($titles);
        $("#ddform").attr("action", basePath+'monitorCenter/exportTrackTable.action');
		$("#ddform").submit();
}

function getTitles(){
	var $table=$("#tbTrack");
	var $td=$table.find("td");
	var len=$td.length;
	var titles="";
	for(var i=0;i<len;i++){
		titles+=$td.eq(i).html()+",";
	}
	return titles.substring(0,titles.length-1);
}

function getTableData(){
	var $table=$("#mytable");
	var $tr=$table.find("tr");
	var len=$tr.length;
	var tableData="";
	for(var i=0;i<len;i++){
		var $td=$tr.eq(i).find("td");
		for(var j=0;j<$td.length;j++){
			tableData+=($td.eq(j).attr("data-value") || "")+"#";
		}
		tableData+="&";
	}
	return tableData;
}


			height: 320     // 信息窗口高度  
		}
	/**点击弹出**/
	currentCarMarker.addEventListener("click", function(){
		var infoWindow = new BMap.InfoWindow( createInfoWindow(obCar),opts);
		/**设置快捷标题**/
		infoWindow.setTitle(getTitleDiv(obCar.no,obCar.vehicleId,1));
		this.openInfoWindow(infoWindow); 
	});
	

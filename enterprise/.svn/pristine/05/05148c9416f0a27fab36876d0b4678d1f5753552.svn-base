            </isNotEmpty>
            
            <isNotEmpty property="PC_NAME" prepend="and">
      			C.PC_NAME like '%$PC_NAME$%'
            </isNotEmpty>
            
             <isNotEmpty property="sendTimeStart" prepend="and">
      			C.SENDTIME >=#sendTimeStart#
            </isNotEmpty>
            
            <isNotEmpty property="sendTimeEnd" prepend="and">
      			 #sendTimeEnd#>=C.SENDTIME
            </isNotEmpty>
		</dynamic>
	</select>

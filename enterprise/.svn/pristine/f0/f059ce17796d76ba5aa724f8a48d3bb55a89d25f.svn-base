*{ margin:0; padding:0;}

.tab-hd { float:left;margin:8px 0px 0px 10px ;background:#60b90f; height:20px; overflow:hidden; zoom:1;  border:solid 0px red; }
/**vertical-align:middle; text-align:center;**/
.tab-hd div{ cursor:hand; cursor:pointer; float:left;  width:150px; height:20px;   border:solid 0px red; color:#fff; text-align:center; }
.tab-hd div em{ border:solid 0px red;    float:none; font:12px/1.6 Arial; vertical-align:middle; text-align:center;}
.tab-hd div span{ border:solid 0px red;  position:relative; float:right;}
.tab-hd div.active{ background:#fe5e06; cursor:hand; cursor:pointer;}





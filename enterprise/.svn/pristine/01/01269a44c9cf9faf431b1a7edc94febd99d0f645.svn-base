*/ /*  main */
table.form {
	width: 100%;
	border-collapse: collapse; /* 边框重叠 */
	text-align: center;
}

table.form th,table.form th {
	border: none;
	text-align: center;
	FONT-FAMILY: "微软雅黑";
	FONT-SIZE: 12px;
}

table.form  th {
	text-align: center;
	border-style: solid;
	border-width: 1px;
	border-color: #1D82D2 #1D82D2 #1D82D2 #1D82D2;
	background-color: #A0CDF4;
	padding: 2px 10px;
	height: 24px;
}

table.form tbody {
	background: #ffffff;
}

table.form tbody td {
	padding: 5px 10px;
	border: 1px solid #1D82D2;
	border-collapse: collapse;
}

table.form thead td {
	background: url(../images/tb_top.png);
	background-color: #FFFFFF;
}

tr.odd {
	background-color: #ffffff;
}

tr.tableRowEven,tr.even {
	background-color: #EDF3FF;
}

tr.visite {
	background-color: #CBA718;
}

.left {
	text-align: right;
}

.right {
	text-align: left;
	padding-left: 10px
}

.red {
	color: red;
	padding-right: 5px
}

.input {
	width: 200px;
	height: 23px;
	line-height: 23px;
	color: #000;
	font-size: 12px;
	border-color: #1D82D2
}

.text {
	border: 0px solid #A3BEE8;
	background: #ffffff;
	color: #054e86;
	height: 15px;
	font: 12px/ 120% Verdana, Arial, sans-serif, 宋体;
}

.textGreen {
	color: #669900
}

.textGray {
	color: #999999
}

.openDivShow {
	display: none;
}

.showEro {
	display: none;
	color: red;
	font-size: 16px;
}

.openDivShow img {
	display: none;
}

img {
	vertical-align: middle;
}

.mustfill {
	color: red;
	font-family: '宋体';
	font-size: 11px;
}

.tdLeft {
	text-align: right;
	background-color: #D0DEF8;
}

.tdRight {
	text-align: left;
	background-color: #E8EEF7;
}


	<!-- 新增 -->
	<insert id="addProveInfoSQL" parameterClass="java.util.HashMap">
	     INSERT INTO Ent_ProveInfo
           ([VehicleID]
           ,[ProveNameID]
           ,StartTime
           ,EndTime
           ,ProveRate
           ,ProveAddress
           ,ManageArea
           ,Principal
           ,ProveHuman
           ,Remark
           ,EntryTime
           )
	     VALUES

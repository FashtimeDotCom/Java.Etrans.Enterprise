package com.etrans.bubiao.http;

/**
 * 请求参数的Key 常量类
 * @author Ivan
 * @version 1.0
 */
public class ParamKey
{

 
 
 /**
  * 插表时的参数名称
  */
 public static  final String INSERT_PARAM ="insertParam";

 /**
  *  Set 参数集合
  */
 public static final String SET_PARAM="setParam";
 
 
 /**
  *  Where  条件参数集合
  */
 public static final String WHERE_PARAM="whereParam";
 
 
 /**
  *  排序 参数集合
  */
 public static final String ORDER_PARAM="orderParam";
 
 
 /**
  * 操作对象名称(表/视图/存储等的名称)
  */
 public static final String TABLE_NAME="tableName";
 
 /**
  * 记录总数操作对象名称(表/视图/存储等的名称)
  */
 public static final String TOTAL_NAME="totalName";
 
 /**
  * 字段名=值|字段名=值等以 | 隔开的参数字符串，做为待更新的字段以及字段新值
  */
 public static final String PARAM_STRING="paramString";

 /**
  * 字段名=值|字段名=值等以 | 隔开的参数字符串，做为执行SQL语句中的where的条件
  */
 public static final String WHERE_STRING="whereString";
 
 /**
  * 所要查询的每页的记录数
  */
 public static final String PAGE_SIZE="pageSize";
 
 /**
  * 所要查询的当前页数
  */
 public static final String PAGE="page";
 
 /**
  * 字段名=排序方式|字段名=排序方式等以 | 隔开的参数字符串，做为执行SQL语句中的order排序的条件
  */
 public static final String ORDER_STRING="orderString";
 
 
 
 /**
  * 排序字段名
  */
 public static final String ORDER_NAME="sortname";
 
 
 /**
  * 排序方式
  */
 public static final String ORDER_SORT="sortorder";
 
 
 
}

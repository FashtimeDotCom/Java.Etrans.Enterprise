	}
	if(null==data.paramvalue||data.paramvalue==""){
		alert("数据库参数设置为空！");
	}else{
		paramArray=data.paramvalue.split(',');

//		var str ="TransmitIP=203.88.210.56,TransmitPort=17891,Kind=121,ID=dns:A33296,Channel=0&1&2&3";
//		paramArray=str.split(',');
		

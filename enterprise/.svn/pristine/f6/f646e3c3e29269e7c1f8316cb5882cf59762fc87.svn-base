			alert("数据库参数设置格式不对！");
			return;
		}
		var TransmitIP=paramArray[0].split('=')[1];
		var TransmitPort=paramArray[1].split('=')[1];
		var Kind = paramArray[2].split('=')[1];
		var ID = paramArray[3].split('=')[1];

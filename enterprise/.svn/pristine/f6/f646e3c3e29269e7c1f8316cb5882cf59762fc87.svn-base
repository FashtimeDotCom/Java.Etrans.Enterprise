/**
 * 单车订阅
 * @param {} data
 */
function vehicleCallTrack(vehicleId) {
	var jsonParams = {
		vehicleIdStr : vehicleId,
		datetimes : new Date()
	};
	//ljy获取地图类型
	var mapType = parent.mapFrame.getMapType();
//	alert("地图类型："+mapType);
	$.ajax({
	  	url:"monitorCenter/getGpsInfos.action?mapType="+mapType,
		type:"POST",
		dataType:"json",
		data:jsonParams,
		success : vehicleCallTrackBack
	});
}

/**
 * 单车订阅返回处理
 * @param {} data
 */

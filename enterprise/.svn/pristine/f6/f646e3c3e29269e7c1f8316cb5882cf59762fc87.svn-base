		//alert("TransmitIP:"+TransmitIP+"TransmitPort:"+TransmitPort+"Kind:"+Kind+"ID:"+ID+"Channel:"+Channel);
		var url = basePath+'jiankong/jiankong.jsp?TransmitIP='+TransmitIP+"&Kind="+Kind+"&ID="+ID;

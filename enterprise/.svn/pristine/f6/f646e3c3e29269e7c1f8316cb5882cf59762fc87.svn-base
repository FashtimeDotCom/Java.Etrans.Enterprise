 * DIV设置共享对象
 */
var divSetting = {
		
	/**
	* 默认设置的几个值大小
	*/
	minWidth:0,// 最小宽度
	maxWidth:0,// 最大宽度
	minHeight:0,// 最小高度
	maxHeight:0,// 最高高度
	oriWidth:0,//原宽度
	oriHeight:0,// 原高度
	
	
	/**
	 * DIV或者其他空间大小复位
	 * 
	 * @param divId
	 */
	sizeReset:function(divId){
		$("#"+divId).css("width",divSetting.oriWidth);
		$("#"+divId).css("height",divSetting.oriHeight);
	},
	
	/**
	 * DIV或者其他控件大小最小化
	 * 
	 * @param divId
	 */
	sizeMin:function(divId){
		$("#"+divId).css("width",0);
		$("#"+divId).css("height",0);
	}
};

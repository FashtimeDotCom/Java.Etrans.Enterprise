@charset "utf-8";
/* design by Andy(M) */

/* all */
/* * { padding: 0; margin: 0;border: 0; list-style:none; font-weight: inherit; font-style: inherit; outline:0;} */

body{font-size:12px; font-family: 微软雅黑, Arial, Helvetica, sans-serif; background:#F2F2F2;}

a img, :link img, :visited img {border:0;} 

img{ border:0;}

a{ text-decoration:none;}

.left{ float:left;}

.right{ float:right;}

.clear{ clear:both;}

p{margin-bottom:10px;}


ul,li { margin:0;padding:0; list-style-type:none;list-style-image:none;list-style-position:outside;overflow:hidden;}


/* midCSS */
.main{height:100%; overflow-y:auto; background:#f9f9f9; width:100%;}

#cont_box table{font-size:12px; }



/* instr page  */

table.form {
	width: 100%;
	border-collapse: collapse; /* 边框重叠 */
	text-align: center;
}

table.form th,table.form th {
	border: none;
	text-align: center;
	FONT-FAMILY: "微软雅黑";
	FONT-SIZE: 12px;
}

table.form  th {
	text-align: center;
	border-style: solid;
	border-width: 1px;
	border-color: #1D82D2 #1D82D2 #1D82D2 #1D82D2;
	background-color: #A0CDF4;
	padding: 2px 10px;
	height: 24px;
}

table.form tbody {
	background: #ffffff;
}

table.form tbody td {
	padding: 5px 10px;
	border: 1px solid #1D82D2;
	border-collapse: collapse;
}

table.form thead td {
	background: url(../images/tb_top.png);
	background-color: #FFFFFF;
}
.btnQueryCommand{
	background: url("../imgs/look_up.gif") no-repeat;
	border: 0px;
	width: 64px;
	height: 24px;
	cursor: pointer;
}

public class Command1792Util {
	/**
	 * 起始字头
	 */
	private String starCode;
	/**
	 * 命令字
	 */
	private String commandCode;
	/**
	 * 数据块长度
	 */
	private int dataLength;
	/**
	 * 保留字
	 */

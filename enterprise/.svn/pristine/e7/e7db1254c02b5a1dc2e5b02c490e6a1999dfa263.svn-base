     from( 
           select pv.id
			       ,pv.workUnitID
			       ,pv.RegistrationNo
				   ,prc.name AS RegistrationNOColor
				   ,pf.name AS platformName 
			       ,pw.name AS workUnitName
				   ,pc.Name AS customKindName 

      			C.workUnitName like '%$workUnitName$%'
            </isNotEmpty>
            
            <isNotEmpty property="RecdayStart" prepend="and">
      			C.Recday >=#RecdayStart#
            </isNotEmpty>
            
            <isNotEmpty property="RecdayEnd" prepend="and">
      			 #RecdayEnd#>=C.Recday
            </isNotEmpty>
		</dynamic>
	</select>

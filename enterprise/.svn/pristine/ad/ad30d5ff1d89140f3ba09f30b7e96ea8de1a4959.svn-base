	initRole();
	$("#roleDlg").css("display", "block");
	$("#roleDlg").dialog( {
		width : 450,
		height : 140,
		title : '分配角色'
	});
}

 


//打开一个对话框
function openDialog(id, widthValue, heightValue, titleValue) {
    $("#" + id).css("display", "block");
	$("#" + id).dialog( {
		width : widthValue,
		height : heightValue,
		title : titleValue,
		maximizable : true
	});
}

function updatePassword(id) {

	<!-- 获取所有分析组-->
	<select id="getAnalyseGroupsSql" resultClass="java.util.HashMap">
		SELECT
			ID,AnalyseGroupsName
		FROM 
			etta_basedata.bi_analysegroups
	</select>
</sqlMap>
